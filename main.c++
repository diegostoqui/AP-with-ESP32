// Importing bibs

#include <WiFi.h>
#include <WiFiClient.h>
#include <WebServer.h>
#include <ESPmDNS.h>

// Defining leds ports

#define LED_R 2  // Vermelho
#define LED_Y 4  // Amarelo
#define LED_G 18 // Verde

// Output led values

bool red = 0;
bool yellow = 0;
bool green = 0;

// WiFi to connect - SSID
const char *ssid = "Ema";
//  WiFi to connect - Password
const char *password = "ema4ever";

WebServer server(80);

void setup()
{
  // Set pinmode of leds
  pinMode(LED_R, OUTPUT);
  pinMode(LED_Y, OUTPUT);
  pinMode(LED_G, OUTPUT);

  // Set baudrate of monitor serial

  Serial.begin(115200);

  // Connect with WiFi

  Serial.println();
  Serial.print("Connecting to");
  Serial.println(ssid);

  WiFi.begin(ssid, password);

  // Loop para checar a conex√£o

  while (WiFi.status() != WL_CONNECTED)
  {
    delay(500);
    Serial.print(".");
  }

  // Display connection info

  Serial.println("");
  Serial.println("WiFi connected");
  Serial.println("IP Address:");
  Serial.println(WiFi.localIP());

  // Routes

  server.on("/", handleRoot);
  server.on("/handler", handleRed);
  server.on("/handley", handleYellow);
  server.on("/handleg", handleGreen);

  server.onNotFound(handleNotFound);
  server.begin();
  Serial.println("HTTP server started");
}

void loop()
{
  server.handleClient();
  delay(2);
}

